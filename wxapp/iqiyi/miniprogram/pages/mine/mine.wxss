/* miniprogram/pages/mine/mine.wxss */
.container {
  width: 100%;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  position: absolute;
}
.container-login {
  width: 100%;
  height: 400rpx;
  border-bottom: 20rpx solid #F0F0F0;
}
.container-login .headPic {
  width: 100%;
  height: 40%;
  margin: 50rpx auto;

}
.login {
  position: absolute;
  left: 50%;
  top: 23%;
  transform: translate(-50%,0)
}
.skipTo {
  width: 100%;
  height: 10%;
  border-bottom: 20rpx solid #F0F0F0;
  display: flex;
}
.skipTo .center {
  display: flex;
  margin: auto 10rpx;
  flex: 1;
}
.skipTo .center .skipName {
  font-weight: bold;
  margin-left: 5%;
  display: flex;
  align-items: center;
  justify-content: center
}
.skipTo .center .right-arrow {
  float: right;
  width: 25rpx;
  height: 25rpx;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  position: absolute;
  right: 20rpx;
  transform: rotate(45deg);
  margin-top:10rpx;
  margin-right: 3%
}
